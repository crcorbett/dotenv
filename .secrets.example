# Secrets file - copy to ~/.secrets and fill in values
# This file is sourced by zshrc
# DO NOT commit the actual .secrets file!

# 1Password Service Account Token (for headless VM commit signing)
# Create at: https://my.1password.com/developer-tools/service-accounts
export OP_SERVICE_ACCOUNT_TOKEN=""

# GitHub NPM Token (for private packages)
export GH_NPM_TOKEN=""

# Context7 API Key
export CONTEXT7_API_KEY=""

# Exa API Key
export EXA_API_KEY=""

# Cubitt API Key
export CUBITT_API_KEY=""

# Add other secrets as needed...
